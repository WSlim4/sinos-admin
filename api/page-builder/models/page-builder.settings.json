{
  "kind": "collectionType",
  "collectionName": "page_builders",
  "info": {
    "name": "Page Builder"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Meta": {
      "type": "component",
      "repeatable": true,
      "component": "pages.meta"
    },
    "page_id": {
      "type": "string"
    },
    "page_text": {
      "type": "richtext"
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "pages.cover",
        "pages.block",
        "pages.embed"
      ]
    },
    "page_title": {
      "type": "string"
    }
  }
}
